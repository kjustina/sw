{
  "instances": [
    {
      "pipeline": "p4.*",
      "stage": ".*",
      "mpu": ".*",
      "control": {
        "trace": false,
        "phv-debug": false,
        "phv-error": false
      },
      "capture": {
        "key-data": true,
        "instructions": false
      },
      "settings": {
        "trace-size": "4096",
        "wrap": true
      },
      "description": "this enables tracing for all mpus in all stages in p4 ingress and egress pipelines"
    }
  ]
}
